{
  "name": "expo-starter-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "fix": "pnpm lint --fix && pnpm format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "build": "eas build",
    "build:ios": "eas build --platform ios",
    "build:preview": "eas build --platform ios --profile preview",
    "submit": "eas submit",
    "device:create": "eas device:create",
    "device:list": "eas device:list",
    "ios:logs": "npx react-native log-ios",
    "ios:device": "npx expo run:ios --device",
    "ios:refresh": "npx expo prebuild --platform ios",
    "sync:agents": "bash -c 'printf \"# Agent Rules\\n\\nGenerated from .cursor/rules\\n\\n\" > AGENTS.md && for file in .cursor/rules/*; do printf \"\\n## %s\\n\\n\" \"$(basename \"$file\")\" >> AGENTS.md && cat \"$file\" >> AGENTS.md && printf \"\\n\" >> AGENTS.md; done'"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "expo": "54.0.0-canary-20250830-81bb199",
    "expo-background-fetch": "14.0.5-canary-20250830-81bb199",
    "expo-blur": "15.0.5-canary-20250830-81bb199",
    "expo-build-properties": "1.0.6-canary-20250830-81bb199",
    "expo-constants": "18.0.6-canary-20250830-81bb199",
    "expo-font": "14.0.6-canary-20250830-81bb199",
    "expo-glass-effect": "^0.1.4",
    "expo-haptics": "15.0.5-canary-20250830-81bb199",
    "expo-image": "3.0.5-canary-20250830-81bb199",
    "expo-linear-gradient": "15.0.5-canary-20250830-81bb199",
    "expo-linking": "8.0.6-canary-20250830-81bb199",
    "expo-router": "6.0.0-canary-20250830-81bb199",
    "expo-splash-screen": "31.0.7-canary-20250830-81bb199",
    "expo-status-bar": "3.0.6-canary-20250830-81bb199",
    "expo-symbols": "1.0.5-canary-20250830-81bb199",
    "expo-system-ui": "6.0.5-canary-20250830-81bb199",
    "expo-task-manager": "14.0.5-canary-20250830-81bb199",
    "expo-web-browser": "15.0.5-canary-20250830-81bb199",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "^4.0.3",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.15.4",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.4.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@ocavue/eslint-config": "^3.3.3",
    "@react-native-community/cli": "latest",
    "@testing-library/react-native": "^13.3.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.0",
    "babel-jest": "^30.1.2",
    "dotenv": "^17.2.2",
    "eas-cli": "latest",
    "eslint": "^9.34.0",
    "eslint-config-expo": "10.0.1-canary-20250830-81bb199",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.11",
    "prettier": "^3.6.2",
    "react-dom": "19.1.0",
    "react-native-web": "~0.21.1",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
